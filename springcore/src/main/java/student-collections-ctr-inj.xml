<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="student" class="com.baiju.springcore.Student">
		<constructor-arg>
			<value>1234</value>
		</constructor-arg>

		<constructor-arg>
			<value>Baiju</value>
		</constructor-arg>
		
		<constructor-arg>
			<list>
				<value>Core Java</value>
				<value>Advanced Java</value> 
				<value>Struts</value>
				<value>Hibernate</value>
				<value>Spring</value>
			</list>
		</constructor-arg>
		
		<constructor-arg>
			<set>
				<value>99999999999</value>
				<value>88888888888</value> 
				<value>77777777777</value>
			</set>
	    </constructor-arg>
	    
	    <constructor-arg>
			<map>
				<entry key="Core Java" value="10000"> </entry>
				<entry key="Advanced Java" value="15000"> </entry>
				<entry key="Struts" value="5000"> </entry>
				<entry key="Hibernate" value="7000"> </entry>
			</map>
	    </constructor-arg>
	    
	    <constructor-arg>
			<props>
				<prop key="key1">value1</prop>
				<prop key="key2">value2</prop>
				<prop key="key3">value3</prop>
			</props>
	    </constructor-arg>
	        
	</bean>
</beans>