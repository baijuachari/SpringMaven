<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="student" class="com.baiju.springcore.Student">
		<property name="id" value="1234"/>
		<property name="name" value="Baiju"/>       
		<property name="courses">
			<list>
				<value>Core Java</value>
				<value>Advanced Java</value> 
				<value>Struts</value>
				<value>Hibernate</value>
				<value>Spring</value>
			</list>
	    </property>
		
		<property name="mobileNos">
			<set>
				<value>99999999999</value>
				<value>88888888888</value> 
				<value>77777777777</value>
			</set>
	    </property>
	    
	    <property name="feeDetails">
			<map>
				<entry key="Core Java" value="10000"> </entry>
				<entry key="Advanced Java" value="15000"> </entry>
				<entry key="Struts" value="5000"> </entry>
				<entry key="Hibernate" value="7000"> </entry>
			</map>
	    </property>
	    
	    <property name="properties">
			<props>
				<prop key="key1">value1</prop>
				<prop key="key2">value2</prop>
				<prop key="key3">value3</prop>
			</props>
	    </property>
	    	    
	</bean>
 
</beans>